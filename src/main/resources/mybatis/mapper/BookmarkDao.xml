<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.study.springboot.jdbc.BookmarkDAO">
	<select id="selectDao" resultType="com.study.springboot.jdbc.BookmarkDTO">
		SELECT * from bookmark WHERE user_seq = #{0} AND product_seq = #{1}
	</select>
</mapper>
